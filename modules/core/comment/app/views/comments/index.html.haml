#comment_list
  - @comments.each do |c|
    = render('comments/comment', :comment => c)

%p.pager= will_paginate(@comments, :params => {:controller => 'comments', :action => 'index', :model_type => @comments_model_type, :model_id => @comments_model_id}, :onclick => renderer => AjaxLinkRenderer.new('comment_container'))
